    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "errorhandler": "^1.5.0",
    "es6-shim": "^0.35.3",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "mongoose": "^4.11.5",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "tslib": "^1.7.1",
