{
  "name": "booklibrary.angular",
  "version": "1.0.0",
  "description": "Angular Sample application",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "server": "webpack-dev-server --env=dev --inline --progress --watch --port 8080",
    "start": "npm run server",
    "build": "npm run build:dist",
    "cleanup": "rimraf src/client/app/**/*.ngfactory.ts src/client/app/**/*.js src/client/app/**/*.js.map src/client/app/**/*.metadata.json src/client/app/**/*.ngstyle.ts src/client/app/**/*.ngsummary.json build dist",
    "ngc": "ngc",
    "preaot": "ngc -p tsconfig-aot.json",
    "aot": "npm run cleanup && npm run ngc && webpack --env=aot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanmacgowan/BookLibrary.Angular.git"
  },
  "keywords": [],
  "author": "Alan Macgowan",
  "license": "ISC",
  "homepage": "https://github.com/alanmacgowan/BookLibrary.Angular#readme",
  "devDependencies": {
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/node": "^6.0.38",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.1.2",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "codelyzer": "^3.1.2",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "css-to-string-loader": "^0.1.3",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.4",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-helpers": "^1.1.1",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.3.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "~4.2.6",
    "@angular/compiler": "~4.2.6",
    "@angular/core": "~4.2.6",
    "@angular/forms": "~4.2.6",
    "@angular/http": "~4.2.6",
    "@angular/platform-browser": "~4.2.6",
    "@angular/platform-browser-dynamic": "~4.2.6",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "~4.2.6",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
