{
  "name": "booklibrary.angular",
  "version": "2.0.0",
  "description": "Angular Sample application",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "dev": "webpack-dev-server --env=dev --inline --progress --watch --port 8080",
    "start": "set NODE_ENV=development&& concurrently \"npm run dev\" \"npm run server\" ",
    "server": "nodemon --inspect src/server/server.js",
    "build": "npm run build:dist",
    "cleanup": "rimraf src/client/*.js src/client/*.js.map src/client/app/**/*.ngfactory.ts src/client/app/**/*.js src/client/app/**/*.js.map src/client/app/**/*.metadata.json src/client/app/**/*.ngstyle.ts src/client/app/**/*.ngsummary.json build aot",
    "ngc": "ngc",
    "preaot": "npm run cleanup && ngc tsconfig-aot.json",
    "aot": "npm run ngc -p && webpack --env=aot",
    "postaot": "npm run cleanup",
    "test": "karma start ./karma/karma.conf.js",
    "coveralls": "coveralls",
    "precommit": "npm run build",
    "commit": "npm --no-git-tag-version version patch -f && git add . && git commit -m ",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanmacgowan/BookLibrary.Angular.git"
  },
  "keywords": [],
  "author": "Alan Macgowan",
  "license": "ISC",
  "homepage": "https://github.com/alanmacgowan/BookLibrary.Angular#readme",
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.2",
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/jasmine": "^2.5.54",
    "@types/node": "^6.0.38",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.1.2",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "codelyzer": "^3.1.2",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "css-to-string-loader": "^0.1.3",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "generate-changelog": "^1.3.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.15",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.1",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.3.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.5.1",
